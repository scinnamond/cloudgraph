    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>EnumerationQueueBean</managed-bean-name>
    <managed-bean-class>org.cloudgraph.web.model.configuration.EnumerationQueueBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
