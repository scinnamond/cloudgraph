    <managed-bean-class>org.cloudgraph.web.model.data.InstanceEditBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
   
  <managed-bean>
    <managed-bean-name>PersonalizeQueueBean</managed-bean-name>
    <managed-bean-class>org.cloudgraph.web.model.data.PersonalizeQueueBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
   
    <managed-bean>
		<managed-bean-name>NavigationBean</managed-bean-name>
		<managed-bean-class>org.cloudgraph.web.model.navigation.TreeNavigationBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
    <managed-bean>
		<managed-bean-name>ControlNavigationBean</managed-bean-name>
		<managed-bean-class>org.cloudgraph.web.model.navigation.ControlNavigationBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
    <managed-bean>
		<managed-bean-name>OrganizationTreeBean</managed-bean-name>
		<managed-bean-class>org.cloudgraph.web.model.navigation.OrganizationTreeBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

  <managed-bean>
    <managed-bean-name>DataListBean</managed-bean-name>
    <managed-bean-class>org.cloudgraph.web.model.data.DataListBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
