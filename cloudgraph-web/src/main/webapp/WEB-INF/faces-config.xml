<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN" "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config xmlns="http://java.sun.com/JSF/Configuration">
  <!-- ================ Application Resources ================================== -->
  <application>
    <locale-config>
      <default-locale>en</default-locale>
      <supported-locale>de</supported-locale>
    </locale-config>
    <!--
    <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
    -->
  </application>

  <!-- ================ Managed Beans =========================================== -->
  
  <!-- ReferenceDataCache, Scope:application -->
  <managed-bean>
    <managed-bean-name>ReferenceDataCache</managed-bean-name>
    <managed-bean-class>org.cloudgraph.web.model.cache.ReferenceDataCache</managed-bean-class>
    <managed-bean-scope>application</managed-bean-scope>
  </managed-bean>

  <!-- UserBean, Scope:session -->
  <managed-bean>
    <managed-bean-name>UserBean</managed-bean-name>
    <managed-bean-class>org.cloudgraph.web.model.profile.UserBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  
  <!-- ErrorHandlerBean, Scope:session -->
  <managed-bean>
    <managed-bean-name>ErrorHandlerBean</managed-bean-name>
    <managed-bean-class>org.cloudgraph.web.ErrorHandlerBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <!-- SearchBean, Scope:session -->
  <managed-bean>
    <managed-bean-name>SearchBean</managed-bean-name>
    <managed-bean-class>org.cloudgraph.web.model.search.SearchBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  
  <managed-bean>
