    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>InstanceBrowserQueueBean</managed-bean-name>
    <managed-bean-class>org.cloudgraph.web.model.data.InstanceBrowserQueueBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
	<converter>
	    <converter-id>PropertyConverter</converter-id>
	    <converter-class>org.cloudgraph.web.converter.PropertyConverter</converter-class>
	</converter>


    <!-- ================ Navigation Rules =========================================== -->

    <navigation-rule>
      <navigation-case>
        <from-outcome>errorhandler</from-outcome>
        <to-view-id>/ErrorHandler.jsp</to-view-id>
      </navigation-case>
    </navigation-rule>

    <!-- anonymous nav actions -->
    <navigation-rule>
      <navigation-case>
        <from-outcome>topnav_documentation</from-outcome>
        <to-view-id>/documentation/Documentation.jsp</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>topnav_download</from-outcome>
        <to-view-id>/download/Download.jsp</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>topnav_demo</from-outcome>
        <to-view-id>/demo/Demo.jsp</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>topnav_data</from-outcome>
        <to-view-id>/data/Data.jsp</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>topnav_configuration</from-outcome>
        <to-view-id>/configuration/Configuration.jsp</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>topnav_news</from-outcome>
        <to-view-id>/news/News.jsp</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>topnav_email</from-outcome>
        <to-view-id>/email/Email.jsp</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>topnav_blog</from-outcome>
        <to-view-id>/blog/Blog.jsp</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>
      <navigation-case>
        <from-outcome>topnav_administration</from-outcome>
        <to-view-id>/administration/Administration.jsp</to-view-id>
      </navigation-case>
    </navigation-rule>
    <navigation-rule>                                                                               
	    <description></description>
	    <navigation-case>                                                                           
	        <from-outcome>edit_taxonomy</from-outcome>                                              
	        <to-view-id>/administration/TaxonomyEdit.jsp</to-view-id>                      
	    </navigation-case>                                                                          
    </navigation-rule>                                                                              
    
    <!-- non-anonymous nav actions -->                                                                            
     <navigation-rule>                                                                               
	    <description></description>
	    <from-view-id>/data/Data.jsp</from-view-id>                        
	    <navigation-case>                                                                           
	        <from-outcome>edit</from-outcome>                                              
	        <to-view-id>/administration/Administration.jsp</to-view-id>                      
	    </navigation-case>                                                                          
	    <navigation-case>                                                                           
	        <from-outcome>create</from-outcome>                                              
	        <to-view-id>/administration/Administration.jsp</to-view-id>                      
	    </navigation-case>                                                                          
    </navigation-rule>                                                                              
    <navigation-rule>                                                                               
	    <description></description>
	    <from-view-id>/configuration/Configuration.jsp</from-view-id>                        
	    <navigation-case>                                                                           
	        <from-outcome>edit</from-outcome>                                              
	        <to-view-id>/administration/Administration.jsp</to-view-id>                      
	    </navigation-case>                                                                          
	    <navigation-case>                                                                           
	        <from-outcome>create</from-outcome>                                              
	        <to-view-id>/administration/Administration.jsp</to-view-id>                      
	    </navigation-case>                                                                          
    </navigation-rule>                                                                              
    <navigation-rule>                                                                               
	    <description></description>
	    <from-view-id>/administration/Administration.jsp</from-view-id>                        
	    <navigation-case>                                                                           
	        <from-outcome>edit</from-outcome>                                              
	        <to-view-id>/administration/Administration.jsp</to-view-id>                      
	    </navigation-case>                                                                          
	    <navigation-case>                                                                           
	        <from-outcome>create</from-outcome>                                              
	        <to-view-id>/administration/Administration.jsp</to-view-id>                      
	    </navigation-case>                                                                          
    </navigation-rule>                                                                              
    <navigation-rule>                                                                               
	    <description></description>
	    <from-view-id>/campaign/Campaign.jsp</from-view-id>                        
	    <navigation-case>                                                                           
	        <from-outcome>edit</from-outcome>                                              
	        <to-view-id>/administration/Administration.jsp</to-view-id>                      
	    </navigation-case>                                                                          
	    <navigation-case>                                                                           
	        <from-outcome>create</from-outcome>                                              
	        <to-view-id>/administration/Administration.jsp</to-view-id>                      
	    </navigation-case>                                                                          
    </navigation-rule>                                                                              


    <!-- ================ Custom Components =========================================== -->
    <component>
      <component-type>org.cloudgraph.web.component.ChartComponent</component-type>
      <component-class>org.cloudgraph.web.component.ChartComponent</component-class>
    </component>
    <component>
      <component-type>org.cloudgraph.web.component.ChartManagerComponent</component-type>
      <component-class>org.cloudgraph.web.component.ChartManagerComponent</component-class>
    </component>

    <!-- ================ LifeCycle Phase Listeners =========================================== -->
    <lifecycle>
    	<phase-listener>org.cloudgraph.web.component.ChartListener</phase-listener>
    </lifecycle>

 
</faces-config>
